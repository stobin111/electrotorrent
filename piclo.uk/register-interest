<script>
$(function(){
  $('.js-survey-send').addClass('disabled');
  $('.survey input').on('change',function(){
    $('.checked').toggleClass('checked');
    $('input:checked').parent().addClass('checked');
    $('.js-survey-send').removeClass('disabled');
  });
  $('.js-survey-send').on('click',function(){
    if($('form input').is(':checked')){
      window.location.href = $('input:radio:checked').data('url');;
    }
  });
})
</script>


